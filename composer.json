{
    "name": "npmweb/tinymce",
    "description": "TinyMCE available as a Composer Component",
    "type": "component",
    "license": "GPL-2.0+",
    "require": {
        "robloach/component-installer": "*"
    },
    "extra": {
        "component": {
            "scripts": [
                "js/tinymce/tinymce.min.js"
            ],
            "files": [
                "js/tinymce/langs/*",
                "js/tinymce/plugins/*",
                "js/tinymce/skins/*",
                "js/tinymce/themes/*"
            ]
        }
    }
}
